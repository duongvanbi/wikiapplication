<%= form_with model: categorie do |form| %>
    <div>
      <%= form.label :title %><br>
      <%= form.text_field :title %>
      <% categorie.errors.full_messages_for(:title).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.label :description %><br>
      <%= form.text_area :description %><br>
      <% categorie.errors.full_messages_for(:description).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
    <div>
      <%= form.label :types %><br>
      <%= form.text_area :types %><br>
      <% categorie.errors.full_messages_for(:types).each do |message| %>
        <div><%= message %></div>
      <% end %>
    </div>
  
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  